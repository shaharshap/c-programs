#include<stdio.h>
int main()
{
	char s[20];
	printf("enter a string \n");
	gets(s);
	int i,count=0;icount=0,num=0;
	for(i=0;s[i]!='\0';i++)
	{
		if(isupper(s[i]))
		{
			count++;
		}
		else if(islower(s[i]))
		{
			icount++;
		}
		else if(isdigit(s[i]))
		{
			num++;
		}
	}
	printf("count of upper case:%d\n",count);
	printf("count of lower case:%d\n",icount);
	printf("count of numbers:%d",num);
	return 0;
}
input:
enter a string
Aditya@123
output:
count of upper case:1
count of lower case:5
count of numbers:3
